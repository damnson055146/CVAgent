# SiliconFlow API 配置
SILICONFLOW_API_KEY=your_api_key_here
SILICONFLOW_API_KEY_2=your_second_api_key_here

# API 限制配置
SILICONFLOW_KEY1_RPM=60
SILICONFLOW_KEY1_TPM=90000
SILICONFLOW_KEY2_RPM=60
SILICONFLOW_KEY2_TPM=90000

# 模型配置
SILICONFLOW_MODEL=deepseek-ai/DeepSeek-V3
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1

# 重试和熔断配置
SILICONFLOW_ESTIMATED_TOKENS_PER_CALL=1500
SILICONFLOW_RETRY_MAX_ATTEMPTS=3
SILICONFLOW_RETRY_BASE_DELAY_MS=200
SILICONFLOW_CIRCUIT_BREAK_THRESHOLD=3
SILICONFLOW_CIRCUIT_BREAK_SECONDS=60
