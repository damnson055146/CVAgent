# healthy check
SILICONFLOW_CIRCUIT_BREAK_THRESHOLD=3
SILICONFLOW_CIRCUIT_BREAK_SECONDS=60

# api key dispatch
SILICONFLOW_API_KEY=""
SILICONFLOW_KEY1_RPM=60
SILICONFLOW_KEY1_TPM=90000
# 第二个硅基流动Key（可留空）
SILICONFLOW_API_KEY_2=""
SILICONFLOW_KEY2_RPM=60
SILICONFLOW_KEY2_TPM=90000
SILICONFLOW_ESTIMATED_TOKENS_PER_CALL=1500

# switch and retry
SILICONFLOW_RETRY_MAX_ATTEMPTS=3
SILICONFLOW_RETRY_BASE_DELAY_MS=200

SILICONFLOW_MODEL=deepseek-ai/DeepSeek-V3
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1

# OpenAI/ChatGPT 提供商（可配置两个Key；若留空则不启用）
OPENAI_API_KEY=""
OPENAI_KEY1_RPM=60
OPENAI_KEY1_TPM=90000
# 第二个 OpenAI Key（可留空）
OPENAI_API_KEY_2=""
OPENAI_KEY2_RPM=60
OPENAI_KEY2_TPM=90000
OPENAI_BASE_URL=https://api.openai.com/v1

# 数据库配置
POSTGRES_DB=cvagent
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql+psycopg2://postgres:postgres@postgres:5432/cvagent
DATABASE_URL_ASYNC=postgresql+asyncpg://postgres:postgres@postgres:5432/cvagent

ORIGINS=http://localhost:5173,http://127.0.0.1:5173,http://localhost:5174,http://127.0.0.1:5174,http://localhost:5175,http://127.0.0.1:5175

# JWT & security
SECRET_KEY=change-me-in-prod
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440
REFRESH_TOKEN_EXPIRE_DAYS=7

# 日志级别
LOG_LEVEL=INFO

# Redis缓存配置
REDIS_URL=redis://localhost:6379/0
REDIS_TTL=3600

# 缓存配置
SOFT_CACHE_SIZE=1000
HARD_CACHE_TTL=86400

# 用户画像配置
USER_PROFILE_ALIGNMENT_ENABLED=true

